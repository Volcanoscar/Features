<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
        android:title="@string/led_light_settings">

      <PreferenceCategory android:key="led_light_category"
            android:title="@string/led_light_settings">

         <SwitchPreference
                android:key="led_light_settings_low_power_remind"
                android:title="@string/led_light_settings_low_power_remind"
                android:summary="@string/led_light_settings_low_power_remind_desc"/>

         <SwitchPreference
                android:key="led_light_settings_charging_remind"
                android:title="@string/led_light_settings_charging_remind"
                android:summary="@string/led_light_settings_charging_remind_desc"/>

         <SwitchPreference
                android:key="led_light_settings_notification_remind"
                android:title="@string/led_light_settings_notification_remind"
                android:summary="@string/led_light_settings_notification_remind_desc" />
    </PreferenceCategory>
</PreferenceScreen>